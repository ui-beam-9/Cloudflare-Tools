# DNS管理器所需配置
CLOUDFLARE_API_TOKEN=你的Cloudflare API令牌

# 多个域名配置（JSON格式，可配置多个域名）
# 格式：{"域名1": "区域ID1", "域名2": "区域ID2", ...}
CLOUDFLARE_ZONES={"example.com": "区域ID1", "example.org": "区域ID2"}

# R2管理器所需配置
CLOUDFLARE_ACCOUNT_ID=你的Cloudflare账户ID
CLOUDFLARE_R2_ACCESS_KEY_ID=你的R2访问密钥ID
CLOUDFLARE_R2_SECRET_ACCESS_KEY=你的R2私有访问密钥
CLOUDFLARE_R2_BUCKET_NAME=你的R2存储桶名称
R2_ENDPOINT_URL=你的Endpoint_URL
CLOUDFLARE_CUSTOM_DOMAIN=你的自定义域名(可选)
R2_PUBLIC_DOMAIN=你的R2公共域名(可选)

# Cloudflare R2 配置
# 你的 Cloudflare 账户 ID
R2_ACCOUNT_ID=your_account_id
# R2 访问密钥 ID（从 R2 > 管理 API 令牌获取）
R2_ACCESS_KEY_ID=your_access_key_id
# R2 访问密钥密文
R2_ACCESS_KEY_SECRET=your_access_key_secret
# R2 存储桶名称
R2_BUCKET_NAME=your_bucket_name
# R2 端点 URL（格式：https://<account_id>.r2.cloudflarestorage.com）
R2_ENDPOINT_URL=https://your_account_id.r2.cloudflarestorage.com
域名（可选，用于生成公开访问链接）
R2_CUSTOM_DOMAIN=your_custom_domain
# R2 公共域名（可选，用于生成 R2.dev 访问链接）
R2_PUBLIC_DOMAIN=your_public_domain