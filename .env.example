# DNS管理器所需配置
CLOUDFLARE_API_TOKEN=你的Cloudflare API令牌
# 多个域名配置（JSON格式，可配置多个域名）
# 格式：{"域名1": "区域ID1", "域名2": "区域ID2", ...}
CLOUDFLARE_ZONES={"example.com": "区域ID1", "example.org": "区域ID2"}

# Cloudflare R2 配置

# 你的 Cloudflare 账户 ID
R2_ACCOUNT_ID=your_account_id
# R2 访问密钥 ID（从 R2 > 管理 API 令牌获取）
R2_ACCESS_KEY_ID=your_access_key_id
# R2 访问密钥密文
R2_ACCESS_KEY_SECRET=your_access_key_secret
# R2 端点 URL（格式：https://<account_id>.r2.cloudflarestorage.com）
R2_ENDPOINT_URL=https://your_account_id.r2.cloudflarestorage.com
# 多个存储桶配置（JSON格式，可配置多个存储桶）
R2_BUCKETS={"bucket1":{"bucket_name":"存储桶1的名称","custom_domain":"存储桶1的自定义域名（可选）","public_domain":"存储桶2的自定义域名（可选）"},"bucket2":{"bucket_name":"存储桶2的名称","custom_domain":"存储桶1的自定义域名（可选）","public_domain":"存储桶2的自定义域名（可选）"}}